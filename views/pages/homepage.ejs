<!DOCTYPE html>
<html lang="en">

<head>
    <%- include('../partials/head.ejs') %>
        <title>Home Page</title>
</head>

<body>
    <!-- nav -->
    <%- include('../partials/nav.ejs') %>
    
        <h3>Welcome</h3>
        <p>To see all the User details click on name. </p>
        <!-- displaying all the users with schedules -->
        
        <table class="table">
            <thead>
                <tr>
                    <th scope="col">User Id</th>
                    <th scope="col">First Name</th>
                    <th scope="col">Start Time</th>
                    <th scope="col">End Time</th>
                </tr>
            </thead>
            <tbody>
                <% users.forEach(user=> { %>
        
                    <tr>
                        <td>
                            <%= user.users_id %>
                        </td>
                        <td>
                            <%= user.firstname %>
                        </td>
                        <td>
                            <%= user.start_time %>
                        </td>
                        <td>
                            <%= user.end_time %>
                        </td>
                    </tr>
                    <% }) %>
        
        
            </tbody>
        </table>
        
        
        <!-- footer -->
        <%- include('../partials/footer.ejs') %>
</body>

</html>